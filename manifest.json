{
    "name": "Story survey",
    "version": "0.01",
    "description": "User survey on contextual story",
    "background": {
        "scripts": [
            "js/jquery-3.5.1.min.js",
            "background.js"
        ],
        "persistent": false
    },
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": [
            "js/jquery-3.5.1.min.js",
            "content.js"
        ],
        "css": [
            "css/survey.css"
        ]
    }],
    "page_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab"
    ],
    "icons": {
        "16": "images/logo-16.jpg",
        "32": "images/logo-32.jpg",
        "48": "images/logo-48.jpg",
        "128": "images/logo-128.jpg"
    },
    "manifest_version": 2
}